{
  "name": "github-repo-comparer",
  "displayName": "Github repo comparer",
  "version": "0.0.1",
  "description": "A comparer for Github Repositories.",
  "author": "Harry Wu<xiaohanwu12@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
